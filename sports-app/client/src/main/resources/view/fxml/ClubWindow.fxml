<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ClubWindowController">

    <Button fx:id="createEventButton" layoutX="305.0" layoutY="53.0" mnemonicParsing="false" onAction="#OpenCreateNewEventWindow" prefHeight="31.0" prefWidth="110.0" text="Create Event" textAlignment="CENTER">
        <font>
            <Font size="15.0" />
        </font>
    </Button>

   <ListView fx:id="eventList" layoutX="25.0" layoutY="53.0" prefHeight="327.0" prefWidth="244.0" />
   <Label layoutX="25.0" layoutY="24.0" prefHeight="27.0" prefWidth="120.0" text="Your Events">
      <font>
         <Font size="18.0" />
      </font>
   </Label>
   <Button fx:id="deleteEventButton" layoutX="307.0" layoutY="96.0" mnemonicParsing="false" onAction="#OpenDeleteEventWindow" prefHeight="31.0" prefWidth="110.0" text="Delete Events">
      <font>
         <Font size="15.0" />
      </font>
   </Button>
   <Button fx:id="refreshClubWindowButton" layoutX="315.0" layoutY="201.0" mnemonicParsing="false" onAction="#refreshClubWindow" prefWidth="93.0" text="Refresh">
      <font>
         <Font size="15.0" />
      </font>
  </Button>
    <Button fx:id="generateEventsWindowButton" layoutX="280.0" layoutY="135.0" mnemonicParsing="false" onAction="#generatePreviousEventsArrangedByClub" prefHeight="31.0" prefWidth="163.0" text="See Club's previous Events">
        <font>
            <Font size="15.0" />
        </font>
    </Button>
</AnchorPane>
